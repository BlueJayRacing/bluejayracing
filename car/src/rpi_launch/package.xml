<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>rpi_launch</name>
  <version>0.0.0</version>
  <description>Omnibus launch file for the Raspberry Pi on the car</description>
  <maintainer email="rbiswas4@jhu.edu">Rishibrata Biswas</maintainer>
  <license>MIT License</license>

  <depend>car_description</depend>
  <depend>car</depend>
  <depend>inertial_sense_ros2</depend>
  <depend>udp_data_chunk_server</depend>
  <depend>data_api_http_server</depend>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
