{
  "name": "baja_proto",
  "version": "1.0.0",
  "keywords": "protocol buffers, protobuf, google",
  "authors": [{
    "name": "Travis Chen"
  }],
  "export": {
    "include": [
      "*.c",
      "*.cpp",
      "*.h"
    ]
  },
  "build": {
    "extraScript": "compile_proto.py",
    "includeDir": "include",
    "srcDir": "src",
    "srcFilter": [
      "+<*.c>"
    ]
  },
  "frameworks": "*",
  "platforms": "*"
}